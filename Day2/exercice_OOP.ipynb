    "class Library:\n",
    "    def __init__(self, json_file: str = 'library_data.json'):\n",
    "        self.json_file = json_file\n",
    "        self.books = []\n",
    "        self.users = []\n",
    "        self.load_books()\n",
    "    \n",
    "    def __str__(self) -> str:\n",
    "        return f\"Library with {len(self.books)} books and {len(self.users)} users\"\n",
    "    \n",
    "    def add_book(self, book: Book) -> None:\n",
    "        self.books.append(book)\n",
    "        self.save_books()  # Save after adding a new book\n",
    "    \n",
    "    def add_user(self, user: User) -> None:\n",
    "        self.users.append(user)\n",
    "    \n",
    "    def list_available_books(self) -> None:\n",
    "        available_books = [book for book in self.books if book.available]\n",
    "        if available_books:\n",
    "            print(\"Available books:\")\n",
    "            for book in available_books:\n",
    "                print(f\"- {book}\")\n",
    "        else:\n",
    "            print(\"No books available\")\n",
    "    \n",
    "    def borrow_book(self, user: User, book_title: str) -> bool:\n",
    "        if user not in self.users:\n",
    "            print(\"User not registered in library\")\n",
    "            return False\n",
    "        \n",
    "        for book in self.books:\n",
    "            if book.title == book_title and book.available:\n",
    "                return user.borrow_book(book)\n",
    "        return False\n",
    "    \n",
    "    def return_book(self, user: User, book_title: str) -> bool:\n",
    "        if user not in self.users:\n",
    "            print(\"User not registered in library\")\n",
    "            return False\n",
    "        \n",
    "        for book in user.borrowed_books:\n",
    "            if book.title == book_title:\n",
    "                return user.return_book(book)\n",
    "        return False\n",
    "    \n",
    "    # Bonus: Search function\n",
    "    def search_books(self, title: str) -> list:\n",
    "        return [book for book in self.books if title.lower() in book.title.lower()]\n",
    "    \n",
    "    def save_books(self) -> None:\n",
    "        \"\"\"Save all books to JSON file\"\"\"\n",
    "        books_data = [book.to_dict() for book in self.books]\n",
    "        with open(self.json_file, 'w') as f:\n",
    "            json.dump(books_data, f, indent=4)\n",
    "    \n",
    "    def load_books(self) -> None:\n",
    "        \"\"\"Load books from JSON file\"\"\"\n",
    "        try:\n",
    "            with open(self.json_file, 'r') as f:\n",
    "                books_data = json.load(f)\n",
    "                self.books = [Book.from_dict(book_data) for book_data in books_data]\n",
    "        except FileNotFoundError:\n",
    "            self.books = []"
   ]
